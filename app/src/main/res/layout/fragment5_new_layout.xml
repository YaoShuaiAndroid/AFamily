<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff">

    <com.family.afamily.view.MyScrollView
        android:id="@+id/frag5_my_scroll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:overScrollMode="never"
        android:scrollbars="none"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/frag5_head_bg"
                android:layout_width="match_parent"
                android:layout_height="235dp"
                android:background="@mipmap/user_bg">

                <LinearLayout
                    android:id="@+id/frag5_title2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="visible"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:paddingRight="7dp">

                        <TextView
                            android:id="@+id/frag5_setting_tv2"
                            android:layout_width="40dp"
                            android:layout_height="match_parent"
                            android:layout_centerVertical="true"
                            android:layout_toLeftOf="@+id/frag5_msg_ic2"
                            android:gravity="center"
                            android:layout_marginRight="5dp"
                            android:text="设置"
                            android:textColor="#333"
                            android:textSize="16sp" />

                        <ImageView
                            android:id="@+id/frag5_msg_ic2"
                            android:layout_width="37dp"
                            android:layout_height="match_parent"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:padding="9dp"
                            android:src="@mipmap/ic_user_13" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/frag5_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:paddingRight="7dp">

                        <TextView
                            android:id="@+id/frag5_setting_tv"
                            android:layout_width="40dp"
                            android:layout_height="match_parent"
                            android:layout_centerVertical="true"
                            android:layout_toLeftOf="@+id/frag5_msg_ic"
                            android:gravity="center"
                            android:layout_marginRight="5dp"
                            android:text="设置"
                            android:textColor="#333"
                            android:textSize="16sp" />

                        <ImageView
                            android:id="@+id/frag5_msg_ic"
                            android:layout_width="37dp"
                            android:layout_height="match_parent"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:padding="9dp"
                            android:src="@mipmap/ic_user_13" />
                    </RelativeLayout>
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="7dp"
                    android:layout_above="@+id/frag5_user_nick">
                    <RelativeLayout
                        android:id="@+id/user_head_rl"
                        android:layout_width="77dp"
                        android:layout_height="77dp"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/circle_h_whilte_bg"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:minWidth="90dp"
                        android:maxWidth="150dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/user_car_bg"
                        android:text="探亲卡"
                        android:textColor="#fff"
                        android:gravity="center"
                        android:singleLine="true"
                        android:paddingLeft="7dp"
                        android:paddingRight="7dp"
                        android:ellipsize="end"
                        android:textSize="11sp"
                        android:layout_marginTop="15dp"
                        android:id="@+id/frag5_user_car"
                        android:layout_alignTop="@+id/frag5_user_head"
                        android:layout_alignLeft="@+id/frag5_user_head"
                        android:layout_alignStart="@+id/frag5_user_head"
                        android:visibility="invisible"
                        android:layout_marginLeft="60dp"
                        android:layout_marginStart="60dp" />
                    <TextView
                        android:id="@+id/frag5_user_end"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:minWidth="110dp"
                        android:background="@drawable/user_end_bg"
                        android:paddingLeft="7dp"
                        android:paddingRight="7dp"
                        android:text="2018-02-18到期"
                        android:textColor="#fff"
                        android:gravity="center"
                        android:visibility="invisible"
                        android:textSize="11sp"
                        android:layout_below="@+id/frag5_user_car"
                        android:layout_alignRight="@+id/user_head_rl"
                        android:layout_alignEnd="@+id/user_head_rl"
                        android:layout_marginEnd="60dp"
                        android:layout_marginRight="60dp"/>
                    <ImageView
                        android:id="@+id/frag5_user_head"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:scaleType="fitXY"
                        android:src="@mipmap/tx"
                        />
                </RelativeLayout>

                <TextView
                    android:id="@+id/frag5_user_nick"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/frag5_head_bottom"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="15dp"
                    android:textColor="#fff"
                    android:textSize="18sp" />

                <LinearLayout
                    android:id="@+id/frag5_head_bottom"
                    android:layout_width="match_parent"
                    android:layout_height="57dp"
                    android:layout_alignParentBottom="true"
                    android:background="#40000000"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/frag5_follow_ll"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="关注"
                            android:textColor="#fff"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/frag5_follow_count"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1.5dp"
                            android:text="0"
                            android:textColor="#fff"
                            android:textSize="15sp" />
                    </LinearLayout>

                    <View
                        android:layout_width="0.8dp"
                        android:layout_height="30dp"
                        android:background="#80ffffff" />

                    <LinearLayout
                        android:id="@+id/frag5_collection_ll"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="收藏"
                            android:textColor="#fff"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/frag5_collection_count"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1.5dp"
                            android:text="0"
                            android:textColor="#fff"
                            android:textSize="15sp" />
                    </LinearLayout>

                    <View
                        android:layout_width="0.8dp"
                        android:layout_height="30dp"
                        android:background="#80ffffff" />

                    <LinearLayout
                        android:id="@+id/frag5_integral_ll"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="积分"
                            android:textColor="#fff"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/frag5_integral_count"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1.5dp"
                            android:text="0"
                            android:textColor="#fff"
                            android:textSize="15sp" />
                    </LinearLayout>

                    <View
                        android:layout_width="0.8dp"
                        android:layout_height="30dp"
                        android:background="#80ffffff" />

                    <LinearLayout
                        android:id="@+id/frag5_money_ll"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="优惠劵"
                            android:textColor="#fff"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/frag5_money_tl"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1.5dp"
                            android:text="0"
                            android:textColor="#fff"
                            android:textSize="15sp" />
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:visibility="gone"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/frag5_integral_tab"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:src="@mipmap/ic_user_9" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="4dp"
                        android:text="积分专区"
                        android:textColor="#777"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/frag5_order_tab"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:src="@mipmap/ic_user_10" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="4dp"
                        android:text="我的订单"
                        android:textColor="#777"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/frag5_shopping_car_tab"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:src="@mipmap/ic_user_11" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="4dp"
                        android:text="购物车"
                        android:textColor="#777"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/frag5_coupon_tab"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:src="@mipmap/ic_user_12" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="4dp"
                        android:text="我的优惠劵"
                        android:textColor="#777"
                        android:textSize="14sp" />
                </LinearLayout>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="87dp"
                android:paddingTop="10dp"
                android:paddingLeft="22dp"
                android:visibility="gone"
                android:paddingRight="22dp"
                >
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@mipmap/ic_user_14"/>
                <RelativeLayout
                    android:id="@+id/frag5_not_open"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:visibility="visible">
                    <TextView
                        android:id="@+id/frag5_not_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="22sp"
                        android:textStyle="bold"
                        android:textColor="#fff"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="15dp"
                        android:text="书房会员年卡"/>
                    <TextView
                        android:id="@+id/frag5_open_vip"
                        android:layout_width="74dp"
                        android:layout_height="22dp"
                        android:layout_below="@+id/frag5_not_tv"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="5dp"
                        android:background="@drawable/frag5_open_vip_bg"
                        android:textColor="#fff"
                        android:textSize="12sp"
                        android:gravity="center"
                        android:text="成为会员"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/frag5_is_vip"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:visibility="gone">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_marginLeft="30dp"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/frag5_vip_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="22sp"
                            android:textStyle="bold"
                            android:textColor="#fff"
                            android:layout_marginTop="15dp"
                            android:text="书房会员年卡"/>
                        <TextView
                            android:id="@+id/frag5_member_data"
                            android:layout_width="wrap_content"
                            android:layout_height="22dp"
                            android:layout_below="@+id/frag5_vip_tv"
                            android:layout_marginTop="5dp"
                            android:textColor="#fff"
                            android:textSize="13sp"
                            android:gravity="center"
                            android:text="2017-12-12 ----- 2018-12-12"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginRight="15dp"
                        android:gravity="right|center_vertical"
                        android:orientation="vertical"
                        android:layout_alignParentRight="true">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="15sp"
                            android:includeFontPadding="false"
                            android:textColor="#c28e28"
                            android:text="RMB"/>
                        <TextView
                            android:id="@+id/frag5_member_money"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:textSize="25sp"
                            android:textColor="#ad7000"
                            android:text="300"/>
                    </LinearLayout>
                </RelativeLayout>
            </RelativeLayout>
            <TextView
                android:id="@+id/frag5_member_over_tv"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_marginLeft="22dp"
                android:layout_marginRight="22dp"
                android:gravity="center"
                android:visibility="gone"
                android:textColor="#555"
                android:textSize="13.5sp"
                android:text="您的会员卡于2017年12月31日到期，去续费"
                android:background="#f1f4f6"/>
            <RelativeLayout
                android:id="@+id/frag5_baby_record_item"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:paddingRight="15dp"
                android:paddingLeft="15dp"
                android:background="@mipmap/ic_user_16">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textSize="15sp"
                    android:textColor="#fff"
                    android:text="宝贝成长记录"/>
                <ImageView
                    android:id="@+id/frag5_arrow_1"
                    android:layout_width="12dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/ic_arrow_white"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textColor="#fff"
                    android:textSize="15sp"
                    android:text="去记录"
                    android:layout_marginLeft="4dp"
                    android:layout_toLeftOf="@+id/frag5_arrow_1"/>
            </RelativeLayout>
            <include layout="@layout/line_10dp_layout"/>
            <RelativeLayout
                android:id="@+id/frag5_my_finance_item"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:visibility="gone"
                >
                <ImageView
                    android:id="@+id/frag5_item_iv1"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/ic_user_1"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/frag5_item_iv1"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp"
                    android:textColor="#333"
                    android:layout_centerVertical="true"
                    android:text="我的财务"/>
                <ImageView
                    android:layout_width="12dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/ic_arrow_g_h"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="15dp"
                    android:layout_centerVertical="true"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.8dp"
                    android:background="@color/line_color"
                    android:layout_alignParentBottom="true"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/frag5_study_record_item"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:paddingLeft="12dp"
                android:paddingRight="15dp">
                <ImageView
                    android:id="@+id/frag5_item_iv2"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/ic_user_2"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/frag5_item_iv2"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp"
                    android:textColor="#333"
                    android:layout_centerVertical="true"
                    android:text="学习记录"/>
                <ImageView
                    android:layout_width="12dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/ic_arrow_g_h"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.8dp"
                android:background="@color/line_color"
                />
            <RelativeLayout
                android:id="@+id/frag5_my_yuyue_item"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:paddingLeft="12dp"
                android:paddingRight="15dp">
                <ImageView
                    android:id="@+id/frag5_item_iv3"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/ic_user_3"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/frag5_item_iv3"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp"
                    android:textColor="#333"
                    android:layout_centerVertical="true"
                    android:text="我的预约"/>
                <ImageView
                    android:layout_width="12dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/ic_arrow_g_h"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.8dp"
                android:background="@color/line_color"
                />
            <RelativeLayout
                android:id="@+id/frag5_my_zhineng_item"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:paddingLeft="12dp"
                android:paddingRight="15dp">
                <ImageView
                    android:id="@+id/frag8_item_iv8"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/frag5_zhin"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/frag8_item_iv8"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp"
                    android:textColor="#333"
                    android:layout_centerVertical="true"
                    android:text="我的先天智能"/>
                <ImageView
                    android:layout_width="12dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/ic_arrow_g_h"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.8dp"
                android:visibility="gone"
                android:background="@color/line_color"
                />
            <RelativeLayout
                android:id="@+id/frag5_jieyue_item"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:paddingLeft="12dp"
                android:visibility="gone"
                android:paddingRight="15dp">
                <ImageView
                    android:id="@+id/frag5_item_iv4"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/ic_user_4"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/frag5_item_iv4"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp"
                    android:textColor="#333"
                    android:layout_centerVertical="true"
                    android:text="我的借阅"/>
                <ImageView
                    android:layout_width="12dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/ic_arrow_g_h"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/frag5_jcbg_item"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:paddingLeft="12dp"
                android:visibility="gone"
                android:paddingRight="15dp">
                <ImageView
                    android:id="@+id/frag5_item_iv4_"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/ic_user_4"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/frag5_item_iv4_"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp"
                    android:textColor="#333"
                    android:layout_centerVertical="true"
                    android:text="我的检测报告"/>
                <ImageView
                    android:layout_width="12dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/ic_arrow_g_h"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>

            <include layout="@layout/line_10dp_layout"/>

            <RelativeLayout
                android:id="@+id/frag5_video_item"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:paddingLeft="12dp"
                android:paddingRight="15dp">
                <ImageView
                    android:id="@+id/frag5_item_iv5"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/ic_user_5"/>
                <TextView
